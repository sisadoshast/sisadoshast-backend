<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ResulteQuestion.
    -->
    <changeSet id="20201125131000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_voter_id"
                                 baseTableName="resulte_question"
                                 constraintName="fk_resulte_question_employee_voter_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="employee_owner_id"
                                 baseTableName="resulte_question"
                                 constraintName="fk_resulte_question_employee_owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="resulte_question"
                                 constraintName="fk_resulte_question_question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>

    </changeSet>
</databaseChangeLog>
